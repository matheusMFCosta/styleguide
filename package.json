{
  "name": "@gocommerce/styleguide",
  "version": "1.0.92",
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "github": "gh-pages -d styleguide",
    "compile": "npm run build && npm run cleanlib && NODE_ENV=production babel --plugins transform-es2015-modules-commonjs ./src --out-dir ./ --ignore '__tests__,*.spec.js,builder.js' --presets=react-app && npm run copycss",
    "build": "NODE_ENV=production babel-node src/builder.js --presets es2015",
    "copycss": "copyfiles -u 2 src/**/*.css lib",
    "cleanlib": "[ -d lib ] && sudo rm -rf lib || echo Nothing to clean!"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "homepage": "https://vtex-gocommerce.github.io/styleguide",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "copyfiles": "^1.2.0",
    "eslint-config-vtex": "^7.0.0",
    "eslint-config-vtex-react": "^3.0.3",
    "eslint-config-vtex-react-app": "^3.1.1",
    "fs": "0.0.1-security",
    "gh-pages": "^1.1.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts": "1.0.17",
    "react-styleguidist": "^6.5.3",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@gocommerce/tachyons": "^1.0.2-gc.16",
    "i18n-country-code": "^1.0.0",
    "npm": "^5.8.0",
    "react-input-mask": "^1.2.2",
    "react-paginate": "^5.2.2",
    "react-responsive-modal": "^2.0.1",
    "react-select": "^2.0.0-beta.2",
    "vtex": "^2.22.0",
    "vtex-tachyons": "3.0.0"
  }
}
